[submodule "nvim"]
    branch = "master"
    update = merge
	path = .config/nvim
	url = git@github.com:kjwsl/nvim-config
[submodule "notes"]
    branch = "master"
    update = merge
	path = notes
	url = git@github.com:kjwsl/notes
[submodule "oh-my-bash"]
	path = submodules/oh-my-bash
	url = https://github.com/ohmybash/oh-my-bash
[submodule "oh-my-fish"]
	path = submodules/oh-my-fish
	url = https://github.com/oh-my-fish/oh-my-fish
[submodule "tpm"]
	path = .config/tmux/plugins/tpm
	url = https://github.com/tmux-plugins/tpm
[submodule "obsidian-vault"]
	path = obsidian-vault
	url = git@github.com:kjwsl/obsidian-vault
